<!DOCTYPE html>
<html>
<head>
  <title>GiftRAPPED</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <script src="http://code.jquery.com/jquery-1.9.1.js"></script>
</head>
<body>

<%= yield %>

<div class="flash">
  <% flash.each do |key, value| %>
    <div class="flash" id="<%= key %>">
      <%= value %>
    </div>
  <% end %>
</div>

</body>
</html>
